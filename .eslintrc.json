{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prefer-const": "error",
    "no-var": "error",
    "no-tabs": "error",
    "eqeqeq": "error"
  }
}
